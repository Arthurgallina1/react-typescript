   // app.post('/signin')\n            console.log(\"api call mock\");\n            const data = {\n                token: \"123\",\n                username: \"arthur\",\n                userId: 1,\n            };\n\n            app.defaults.headers.authorization = `Bearer ${data.token}`;\n            localStorage.setItem(\"@Auth:token\", data.token);\n            localStorage.setItem(\n                \"@Auth:user\",\n                JSON.stringify({ user: data.username, userId: data.userId })\n            );\n\n            setSigned(true);\n            setUser({ name: data.username, id: data.userId });\n            setToken(data.token);\n            setLoading(false);\n            // history.push(\"/dashboard\");\n            console.log(\"oi\");\n        } catch (err) {\n            console.log(err);\n        }\n    }\n\n    return (\n        <AuthContext.Provider\n            value={{ signed, setSigned, signIn, user, loading }}\n        >\n            {children}\n        </AuthContext.Provider>\n    );\n}\n\nfunction useAuth() {\n    const context = useContext(AuthContext);\n\n    return context;\n}\n\nexport { AuthContext, AuthProvider, useAuth };\n","/home/arthur/Desktop/Arthur/Codigos/reactts/src/context/index.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/services/api.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/index.tsx",["87","88","89","90","91","92","93","94","95","96","97"],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/routes/Route.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/styles.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/hooks/useConsole.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/reportWebVitals.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/components/Person.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/components/LayoutEff.tsx",[],{"ruleId":"98","severity":1,"message":"99","line":7,"column":19,"nodeType":"100","messageId":"101","endL