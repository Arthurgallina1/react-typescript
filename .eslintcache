        setSigned(true);\n            setUser({ name: data.username, id: data.userId });\n            setToken(data.token);\n            setLoading(false);\n            // history.push(\"/dashboard\");\n            console.log(\"oi\");\n        } catch (err) {\n            console.log(err);\n        }\n    }\n\n    return (\n        <AuthContext.Provider\n            value={{ signed, setSigned, signIn, user, loading }}\n        >\n            {children}\n        </AuthContext.Provider>\n    );\n}\n\nfunction useAuth() {\n    const context = useContext(AuthContext);\n\n    return context;\n}\n\nexport { AuthContext, AuthProvider, useAuth };\n","/home/arthur/Desktop/Arthur/Codigos/reactts/src/context/index.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/services/api.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/index.tsx",["79","80","81","82","83","84","85"],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/routes/Route.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/styles.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/hooks/useConsole.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/reportWebVitals.ts",[],{"ruleId":"86","severity":1,"message":"87","line":7,"column":19,"nodeType":"88","messageId":"89","endLine":7,"endColumn":26},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"86","severity":1,"message":"94","line":34,"column":11,"nodeType":"88","messageId":"89","endLine":34,"endColumn":18},{"ruleId":"86","severity":1,"message":"95","line":37,"column":12,"nodeType":"88","messageId":"89","endLine":37,"endColumn":17},{"ruleId":"86","severity":1,"message":"96","line":3,"column":8,"nodeType":"88","messageId":"89","endLine":3,"endColumn":18},{"ruleId":"86","severity":1,"message":"97","line":13,"column":5,"nodeType":"88","messageId":"89","endLine":13,"endColumn":23},{"ruleId":"86","severity":1,"message":"98","line":15,"column":5,"nodeType":"88","messageId":"89","endLin