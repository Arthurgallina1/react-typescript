n = `Bearer ${data.token}`;\n            localStorage.setItem(\"@Auth:token\", data.token);\n            localStorage.setItem(\n                \"@Auth:user\",\n                JSON.stringify({ user: data.username, userId: data.userId })\n            );\n\n            setSigned(true);\n            setUser({ name: data.username, id: data.userId });\n            setToken(data.token);\n            setLoading(false);\n            // history.push(\"/dashboard\");\n            console.log(\"oi\");\n        } catch (err) {\n            console.log(err);\n        }\n    }\n\n    return (\n        <AuthContext.Provider\n            value={{ signed, setSigned, signIn, user, loading }}\n        >\n            {children}\n        </AuthContext.Provider>\n    );\n}\n\nfunction useAuth() {\n    const context = useContext(AuthContext);\n\n    return context;\n}\n\nexport { AuthContext, AuthProvider, useAuth };\n","/home/arthur/Desktop/Arthur/Codigos/reactts/src/context/index.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/services/api.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/index.tsx",["83","84","85","86","87","88","89","90","91","92","93"],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/routes/Route.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/pages/Dashboard/styles.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/hooks/useConsole.tsx",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/reportWebVitals.ts",[],"/home/arthur/Desktop/Arthur/Codigos/reactts/src/components/Person.tsx",[],{"ruleId":"94","severity":1,"message":"95","line":7,"column":19,"nodeType":"96","messageId":"97","endLine":7,"endColumn":26},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"94","severity":1,"message":"102","line":34,"column":11,"nodeType":"96","messageId":"97","endLine":34,"endColumn":18},{"ruleId":"94","severity":1,"message":"103","line":37,"column":12,"nodeType":"96","messageId":"97","endLine":37,"endColumn